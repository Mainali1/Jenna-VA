{
  "name": "jenna-va-frontend",
  "version": "1.0.0",
  "description": "Frontend for Jenna Voice Assistant - A commercial-grade desktop voice assistant",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
